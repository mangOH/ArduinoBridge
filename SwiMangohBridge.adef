version: 1.0.0
sandboxed: true
start: auto
executables:
{
    SwiMangohBridge = ( SwiMangohBridgeComponent )
}

processes:
{
    run:
    {
        ( SwiMangohBridge )
    }
		
    envVars:
    {
        LE_LOG_LEVEL=DEBUG	
    }
	
    maxCoreDumpFileBytes: 512K
}

bindings:
{
    SwiMangohBridge.SwiMangohBridgeComponent.dataRouterApi -> SwiMangohDataRouter.dataRouterApi
}

requires:
{
    device: 
    { 
        [rw]	/dev/ttyUSB0 /dev/ttyUSB0
    }
}
